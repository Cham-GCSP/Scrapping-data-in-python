   "source": [
    "import re\n",
    "txt = open('regex_sum_1626858.txt')\n",
    "# Variable for storing the sum\n",
    "total = 0\n",
    " \n",
    "# Iterating through the content\n",
    "# Of the file\n",
    "for line in txt:   \n",
    "    line = line.rstrip()\n",
    "    x = re.findall(\"[0-9]+\", line) \n",
    "    for i in x:\n",
    "        total+= int(i)\n",
    " \n",
    "print(total)"
   ]
